@model CheeseMVC.ViewModels.ViewMenuViewModel
@{
    ViewBag.Title = Model.Menu.Name;
}
<ul>
    @foreach(var item in Model.Items)
    {
        <li>@item.Cheese.Name</li>

    }
</ul>
<a asp-controller="Menu" asp-action="AddItem" asp-route-id="@Model.Menu.ID">Add That Shit</a>
